<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.socks.androiddemo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <application
        android:name="com.socks.androiddemo.ui.base.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity android:name="com.socks.androiddemo.ui.AllActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.socks.androiddemo.ui.SpannableActivityity" />
        <activity android:name="com.socks.androiddemo.ui.OtherActivity" />
        <activity android:name="com.socks.androiddemo.ui.NotifycationActivity" />
    </application>

</manifest>